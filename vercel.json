{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "routes": {
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ],
        "headers": {
            "Access-Control-Allow-Origin": "*"
        }
    }
}   